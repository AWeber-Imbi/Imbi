{
    "name": "imbi",
    "version": "0.1.0",
    "description": "Imbi Service Management Platform UI",
    "main": "index.js",
    "scripts": {
        "build": "webpack -p --progress --config webpack.config.js",
        "dev-build": "webpack --progress -d --config webpack.config.js",
        "dev-sass": "node-sass --importer node_modules/node-sass-import scss/imbi.scss > ../imbi/static/css/imbi.css",
        "eslint": "eslint js/**",
        "sass": "node-sass --importer node_modules/node-sass-import --output-style=compressed scss/imbi.scss > ../imbi/static/css/imbi.css",
        "test": "jest",
        "test-coverage": "jest --coverage",
        "watch": "webpack --progress -d --config webpack.config.js --watch",
        "watch-sass": "node-sass --importer node_modules/node-sass-import --watch -r scss/imbi.scss > ../imbi/static/css/imbi.css"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/AWeber/imbi"
    },
    "author": "Gavin M. Roy",
    "license": "BSD-3-Clause",
    "devDependencies": {
